<?xml version="1.0"?>

<robot name="airhockeybot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find airhockeybot_description)/urdf/materials.xacro"/>
  <xacro:include filename="$(find airhockeybot_description)/urdf/macros.xacro"/>
  <xacro:include filename="$(find airhockeybot_description)/urdf/airhockeybot.gazebo.xacro"/>

  <!-- PUK located in gazebo "world" sdf file -->

  <!-- World, center of global coordinate system-->
  <link name="world">
  </link>

  <!-- plate origin at world origin + 0.772m height -->
  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link" />
    <origin xyz="0 0 0.772"/>
  </joint>

  <!-- table  -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/airhockeybot_table_simulation_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="brown"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="${stl_path}/airhockeybot_table_collision_centered.stl" scale="${stl_scale}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.85461556 0.44271590 	-0.28168013" rpy="0.0 0.0 0.0"/>
      <mass value="21.97056711"/>
      <inertia ixx="4.86877084" ixy="0.00193478" ixz="0.23504361" iyy="9.90057992" iyz="-0.00132030" izz="12.18809493" />
    </inertial>
  </link>

  <joint name="drive_joint" type="fixed">
    <parent link="base_link"/>
    <child link="drive_link" />
    <origin xyz="0 0 0"/>
  </joint>

  <!-- x drive  -->
  <link name="drive_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/airhockeybot_x_drive_simulation.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="0 0.4 -0.4" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.33784705 	0.44480371 	-0.05052649" rpy="0.0 0.0 0.0"/>
      <mass value="1.82959542"/>
      <inertia ixx="0.40444292" ixy="-0.00021855" ixz="-0.00308854" iyy="0.31497748" iyz="-0.00019265" izz="0.71650895" />
    </inertial>
  </link>

  <joint name="front_right_corner_joint" type="fixed">
    <parent link="base_link"/>
    <child link="front_right_corner_link" />
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
  </joint>

  <!-- table corner -->
  <xacro:TableCornerLink pos="front_right"/>

  <joint name="front_left_corner_joint" type="fixed">
    <parent link="base_link"/>
    <child link="front_left_corner_link" />
    <origin xyz="0 0.885 0" rpy="0.0 0.0 ${-pi*0.5}"/>
  </joint>

  <!-- table corner -->
  <xacro:TableCornerLink pos="front_left"/>

  <joint name="rear_right_corner_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rear_right_corner_link" />
    <origin xyz="1.803 0 0" rpy="0.0 0.0 ${pi*0.5}"/>
  </joint>

  <!-- table corner -->
  <xacro:TableCornerLink pos="rear_right"/>

  <joint name="rear_left_corner_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rear_left_corner_link" />
    <origin xyz="1.803 0.885 0" rpy="0.0 0.0 ${pi}"/>
  </joint>

  <!-- table corner -->
  <xacro:TableCornerLink pos="rear_left"/>

  <joint name="plate_joint" type="fixed">
    <parent link="base_link"/>
    <child link="plate_link" />
    <origin xyz="0.901 0.442 -0.023"/>
  </joint>

  <!-- plate  -->
  <link name="plate_link">
    <visual>
      <origin xyz="0.0 0.0 0.025" rpy="0 0 0"/>
      <geometry>
          <mesh filename="${stl_path}/master.dae" scale="18.02 8.84 1"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="1.802 0.884 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.270808667" ixy="0.0" ixz="0.0" iyy="0.0653296667" iyz="0.0" izz="0.335721667"/>
    </inertial>
  </link>

  <joint name="cam_joint" type="fixed">
    <parent link="base_link"/>
    <child link="cam_link" />
    <origin xyz="0.92 0.48 1.27" rpy="${pi*0.5} ${pi*0.5} ${pi}"/>
  </joint>

  <!-- camera  -->
  <link name="cam_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/logitech_C90_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="${stl_path}/logitech_C90_centered.stl" scale="${stl_scale}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.00096633 0.0 0.01146853" rpy="0.0 0.0 0.0"/>
      <mass value="0.04702889"/>
      <inertia ixx="0.00003298" ixy="0.0" ixz="0.00000008" iyy="0.00000439" iyz="0.0" izz="0.00003377" />
    </inertial>
  </link>

  <joint name="basket_joint" type="fixed">
    <parent link="base_link"/>
    <child link="basket_link" />
    <origin xyz="0.0 0.3076245 0.0"/>
  </joint>

  <!-- basket  -->
  <link name="basket_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/basket_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="-0.05 0.1 -0.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.1 0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.01615226 0.12250000 -0.00852924" rpy="0.0 0.0 0.0"/>
      <mass value="0.08063754"/>
      <inertia ixx="0.00044982" ixy="0.0" ixz="0.0" iyy="0.00003527" iyz="0.0" izz="0.00041894" />
    </inertial>
  </link>

  <joint name="rear_basket_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rear_basket_link" />
    <origin xyz="1.802 0.555 0.0" rpy="0.0 0.0 ${pi}"/>
  </joint>

  <!-- rear basket  -->
  <link name="rear_basket_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/basket_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="-0.05 0.1 -0.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.1 0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.01615226 0.12250000 -0.00852924" rpy="0.0 0.0 0.0"/>
      <mass value="0.08063754"/>
      <inertia ixx="0.00044982" ixy="0.0" ixz="0.0" iyy="0.00003527" iyz="0.0" izz="0.00041894" />
    </inertial>
  </link>

  <!-- bot x joint at plate origin -->
  <joint name="joint_x" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="x_link"/>
    <axis xyz="1 0 0"/>
    <dynamics damping="0.01" friction="0.0"/>
    <!-- effort & velocity determine movement behaviour -->
    <limit effort="30" velocity="0.5" lower="0.038" upper="0.855"/>
  </joint>

  <xacro:SimpleTransmission n="1" joint="joint_x" />

  <!-- x traverse  -->
  <link name="x_link">
    <visual>
      <origin xyz="-0.042 0.4425 0.0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/airhockeybot_x_traverse_simulation_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="-0.0625 0.4425 0.02" rpy="0 0 0"/>
      <geometry>
        <box size="0.039 0.9 0.005"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.02172581 0.03052965 0.01843041" rpy="0.00692908 -0.04632697 -0.00049498"/>
      <mass value="0.76968884"/>
      <inertia ixx="0.13185552" ixy="0.00006495" ixz="0.00000198" iyy="0.00052792" iyz="0.00090734" izz="0.13190152"/>
    </inertial>
  </link>

  <joint name="joint_y" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="x_link"/>
    <child link="y_link"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.01" friction="0.0"/>
    <limit effort="30" velocity="0.5" lower="0.0365" upper="0.849"/>
  </joint>

  <xacro:SimpleTransmission n="2" joint="joint_y" />

  <!-- y cart  -->
  <link name="y_link">
    <visual>
      <origin xyz="-0.0626 -0.04 -0.001" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/airhockeybot_y_simulation_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="-0.0626 -0.04 -0.001" rpy="0 0 0"/>
      <geometry>
        <mesh filename="${stl_path}/airhockeybot_y_simulation_centered.stl" scale="${stl_scale}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.00928436 0.04035794 0.04480821" rpy="-0.00355664 0.01354482 -0.05646527"/>
      <mass value="0.15505923"/>
      <inertia ixx="0.00017229" ixy="0.00000269" ixz="-0.00000117" iyy="0.00012496" iyz="-0.00000044" izz="0.00025958"/>
    </inertial>
  </link>

  <joint name="bat_joint" type="fixed">
    <parent link="y_link"/>
    <child link="bat_link" />
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- bat  -->
  <link name="bat_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/puk_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="${stl_path}/puk_centered.stl" scale="${stl_scale}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.01695769" rpy="0.0 0.0 0.0"/>
      <mass value="0.08940609"/>
      <inertia ixx="0.00004276" ixy="0.0" ixz="0.0" iyy="0.00004276" iyz="0.0" izz="0.00004592" />
    </inertial>
  </link>

  <joint name="player_joint_y" type="prismatic">
    <parent link="base_link"/>
    <child link="player_y_link" />
    <origin xyz="1.73 0.442 0.071" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.1" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-0.4" upper="0.4"/>
  </joint>

  <xacro:SimpleTransmission n="3" joint="player_joint_y" /> 

  <!-- player bat y -->
  <link name="player_y_link">
    <!-- we don't want this visualized -->
    <!--<visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <box size="0.02 0.02 0.02"/>
      </geometry>
      <material name="green"/>
    </visual> -->
     <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="0.01"/>
      <inertia ixx="1.7e-5" ixy="0.0" ixz="0.0" iyy="1.7e-5" iyz="0.0" izz="1.7e-5" />
    </inertial>
  </link> 

   <joint name="player_joint_x" type="prismatic">
    <parent link="player_y_link"/>
    <child link="player_x_link" />
    <origin xyz="0.0 0.0 -0.068" rpy="0.0 0.0 0.0"/>
    <axis xyz="1 0 0"/>
    <dynamics damping="0.01" friction="0.0"/>
    <limit effort="30" velocity="1.0" lower="-0.855" upper="0.03"/>
  </joint>

  <xacro:SimpleTransmission n="4" joint="player_joint_x" /> -->

  <!-- player bat x -->
  <link name="player_x_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <mesh filename="${stl_path}/puk_centered.stl" scale="${stl_scale}"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="${stl_path}/puk_centered.stl" scale="${stl_scale}"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.01695769" rpy="0.0 0.0 0.0"/>
      <mass value="0.08940609"/>
      <inertia ixx="0.00004276" ixy="0.0" ixz="0.0" iyy="0.00004276" iyz="0.0" izz="0.00004592" />
    </inertial>
  </link> 

  <!-- Solid cuboid of width w, height h, depth d, and mass m inertia:
         ixx = 1/12m(h^2 + d^2), iyy = 1/12m(w^2 + d^2), izz = 1/12m(w^2 + h^2)-->
  <!-- Solid cylinder of radius r, height h and mass m inertia : ixx = 1/12*m*(3r^2 + h^2), iyy = 1/12*m*(3r^2 + h^2), izz = 1/2mr^2 -->

</robot>